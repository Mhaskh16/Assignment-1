import matplotlib.pyplot as plt

# given data
years = [2011, 2012, 2013, 2014, 2015]
num_students = [1350, 1550, 1250, 1750, 2750]

# create the plot
plt.plot(years, num_students, marker='o')

# set the labels and title
plt.xlabel('Year')
plt.ylabel('Number of Students')
plt.title('Number of Students Who Submitted Their Grades for Recheck')

# display the plot
plt.show()
